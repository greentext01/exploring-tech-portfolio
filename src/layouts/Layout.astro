---
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/png" href="/favicon.png" />
		<meta name="generator" content={Astro.generator} />
		<title>{title} | Olivier's Explorinng Tech Portfolio</title>
	</head>
	<body class="dark:bg-neutral-900 min-w-[350px] bg-green-100/50">
		<slot />
	</body>
</html>
<style>
</style>
